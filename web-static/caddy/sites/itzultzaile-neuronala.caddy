http://itzultzaile-neuronala.zital.eus http://itzultzaile-neuronala.opi5 {
    root * /app/itzultzaile-neuronala/dist

    try_files {path} {path}/ /index.php?{query}

    @php {
        path *.php
    }

    handle @php {
        root * /app/itzultzaile-neuronala/server
        php_fastcgi php-fpm:9000
    }

    file_server

    log {
        output stdout
    }
}
